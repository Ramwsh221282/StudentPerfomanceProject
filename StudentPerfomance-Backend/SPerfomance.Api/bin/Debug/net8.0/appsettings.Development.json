{
  "urls": "http://*:5000",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Error",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "app.log",
      "Append": true,
      "MinLevel": "Warning",
      "FileSizeLimitBytes": 0,
      "MaxRollingFiles": 0
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": ["http://localhost:4200"]
  },
  "PasswordRecoveryFrontend": {
    "Frontend": ["http://localhost:4200"]
  }
}
